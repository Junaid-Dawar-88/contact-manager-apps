datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  group     String
  address   String?
  createdAt DateTime @default(now())
}
